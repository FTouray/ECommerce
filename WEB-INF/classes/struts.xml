<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<package name="default" extends="struts-default">

	<action name="registerUser" class="Registration" method="register">
    <result name="success">/login.jsp</result>
    <result name="error">/index.jsp</result>
</action>

 <action name="loginUser" class="Login" method="login">
        <result name="success">/navigation.jsp</result>
        <result name="error">/login.jsp</result>
    </action>

     <action name="addItem" class="AddItems" method="add">
        <result name="success">/addItem.jsp</result>
        <result name="success">/addItem.jsp</result>
    </action>

    

    <action name="logout" class="Logout" method="execute">
    <result type="redirect">/login.jsp</result>
</action>
		
    </package>
</struts>